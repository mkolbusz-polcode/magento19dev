<h1>Google Map</h1>
<?php if(!empty($this->getApiKey()) && !empty($this->getAddress())): ?>
    <iframe
      width="600"
      height="450"
      frameborder="0" style="border:0"
      src="https://www.google.com/maps/embed/v1/place?key=<?php echo $this->getApiKey(); ?>
        &q=<?php echo $this->getAddress(); ?>" allowfullscreen>
    </iframe>
<?php endif; ?>
